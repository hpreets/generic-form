<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Gift_Count_Score__c</fullName>
    <description>Calculates score based on number of gifts made in previous full calendar year (e.g., if today is 3/1/22, based on number of gifts made in 2021).</description>
    <externalId>false</externalId>
    <formula>CASE (of_Donations_Last_1_Years__c, 0, 0, 1, 3, 2, 3, 3, 2, 4, 2, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>0=0; 1 or 2=3; 3 or 4=2; 5 or more =1</inlineHelpText>
    <label>Gift Count Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
